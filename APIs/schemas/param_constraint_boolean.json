{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Describes a Boolean Parameter Constraint",
  "title": "Boolean Parameter Constraint",
  "type": "object",
  "required": [
    "type"
  ],
  "minProperties": 2,
  "properties": {
    "type": {
      "type": "string",
      "enum": [
        "boolean"
      ]
    },
    "enum": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "boolean"
      }
    }
  }
}
