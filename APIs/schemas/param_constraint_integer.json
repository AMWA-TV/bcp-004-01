{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Describes an Integer Parameter Constraint",
  "title": "Integer Parameter Constraint",
  "type": "object",
  "required": [
    "type"
  ],
  "minProperties": 2,
  "properties": {
    "type": {
      "type": "string",
      "enum": [
        "integer"
      ]
    },
    "enum": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "integer"
      }
    },
    "minimum": {
      "type": "integer"
    },
    "maximum": {
      "type": "integer"
    }
  }
}
